package in.co.rays;

public class arrclasss2 {
	public static void main(String[] args) {
		String[]name = {"uday", "mahi","kanak","abhishek"};
		int count=0;
			for (int i = 0; i < name.length; i++) 
			for (int k = 0; k < name.length; k++) 
			if (name[i].charAt(k)=='k'){
				count++;
				
			}
				System.out.println("count="+count);
				
			}
					
				}
		
	


